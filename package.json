{
  "name": "api-node-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node build.js && tsc --build tsconfig.prod.json",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "sequelize": "--env=production npx sequelize db:migrate",
    "sequelize_dev": "--env=development npx sequelize db:migrate",
    "sequelize_test": "--env=test npx sequelize db:migrate",
    "sequelize_undo": "--env=test npx sequelize db:migrate:undo",
    "sequelize_undo_all": "--env=test npx sequelize db:migrate:undo:all",
    "sequelize_specific": "--env=test npx sequelize db:migrate:undo:all --to XXXXXXXXXXXXXX-create-users.js",
    "seq_seed": "--env=test npx sequelize seed:generate --name users",
    "seq_seed_undo": "--env=test npx sequelize db:seed:undo",
    "seq_seed_undo_all": "--env=test npx sequelize db:seed:undo:all",
    "seq_seed_specific": "--env=test npx sequelize db:seed:undo --seed name-of-seed-as-in-data",
    "migrate": "sequelize db:migrate",
    "apidoc": "apidoc -e /node_modules -e /public -o ./doc node ./src/app"
  },
  "repository": {
    "type": "git",
    "url": "te"
  },
  "author": "Everton Ferreira",
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.2",
    "jsonfile": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.5",
    "session-file-store": "^1.4.0",
    "tslib": "^2.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "^3.5.14",
    "@types/jsonfile": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.1",
    "@types/session-file-store": "^1.2.1",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
    "faker": "^5.1.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jest-cli": "^26.4.2",
    "node-api-doc-generator": "^1.1.1",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "superagent": "^6.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
